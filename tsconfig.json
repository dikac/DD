{
  "compilerOptions": {
    "removeComments" : true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "outDir": "compiled",
    "sourceMap": true,
    "downlevelIteration" : true,
    "lib": [
      "es2016",
      "es2015",
      "dom",
      "es5"
    ]
  },
  "types" : ["jquery"],
  "include":  [
    "src"
  ],
  "exclude":   [
    "src-"
  ]
}
