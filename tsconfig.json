{
  "compilerOptions": {
    "removeComments" : true,
    "moduleResolution": "node",
    "module": "amd",
    "target": "es5",
    "outFile": "compiled/htme.js",
    "sourceMap": true,
    "downlevelIteration" : true,
    "lib": [
      "es2016",
      "es2015",
      "dom",
      "es5"
    ]
  },
  "types" : ["jquery"],
  "include":  [
    "src/Htme/Component/Element/String.ts",
    "src/Htme/Component/Dom/Dom.ts",
    "src/Htme/Component/Datastructure/MapImplement.ts",
    "src/Htme/Component/Datastructure/SetImplement.ts",
    "src/Htme/Plugin/MapPlugin.ts",
    "src/Htme/Component/Element/MapElement.ts",
    "src/Htme/Component/Structure/Panel/Menu/Standard.ts",
    "src"
  ],
  "exclude":   [
    "src-"
  ]

}
